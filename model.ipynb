    "import pandas as pd\n",
    "from sklearn.preprocessing import OneHotEncoder\n",
    "from sklearn.feature_selection import mutual_info_classif\n",
    "from sklearn.model_selection import train_test_split, StratifiedKFold, GridSearchCV\n",
    "from sklearn.ensemble import RandomForestClassifier\n",
    "from sklearn.metrics import classification_report, confusion_matrix, roc_auc_score\n",
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "data = pd.read_csv(\"dataset_sdn.csv\")\n",
    "\n",
    "#Kategorik Değişkenlerin İşlenmesi\n",
    "categorical_cols = ['src', 'dst', 'Protocol']\n",
    "encoder = OneHotEncoder(sparse_output=False, handle_unknown='ignore')\n",
    "encoded_features = encoder.fit_transform(data[categorical_cols])\n",
    "\n",
    "# Yeni özellikleri birleştir\n",
    "encoded_df = pd.DataFrame(encoded_features, columns=encoder.get_feature_names_out(categorical_cols))\n",
    "data = pd.concat([data.drop(categorical_cols, axis=1), encoded_df], axis=1)\n",
    "\n",
    "# Eksik Değerlerin İşlenmesi\n",
    "# Sayısal sütunlarda medyan ile doldurma\n",
    "numerical_cols = data.select_dtypes(include=['float64', 'int64']).columns\n",
    "data[numerical_cols] = data[numerical_cols].fillna(data[numerical_cols].median())\n",
    "\n",
    "# Özellik ve Hedef Değişkenlerin Ayrılması\n",
    "X = data.drop(\"label\", axis=1)  # Özellikler\n",
    "y = data[\"label\"]  # Hedef değişken\n",
    "\n",
    "# Mutual Information ile Özellik Seçimi\n",
    "mutual_info = mutual_info_classif(X, y)\n",
    "feature_scores = pd.Series(mutual_info, index=X.columns)\n",
    "\n",
    "# Önemli özellikleri görselleştir\n",
    "plt.figure(figsize=(12, 6))\n",
    "sns.barplot(x=feature_scores.sort_values(ascending=False).index, \n",
    "            y=feature_scores.sort_values(ascending=False))\n",
    "plt.title(\"Özelliklerin Mutual Information Skorları\")\n",
    "plt.xlabel(\"Özellikler\")\n",
    "plt.ylabel(\"MI Skoru\")\n",
    "plt.xticks(rotation=90)\n",
    "plt.show()\n",
    "\n",
    "# MI skoru > 0.1 olan özellikleri seç\n",
    "important_features = feature_scores[feature_scores > 0.1].index\n",
    "X_selected = X[important_features]\n",
    "\n",
    "# Veri Setinin Bölünmesi\n",
    "X_train, X_test, y_train, y_test = train_test_split(X_selected, y, test_size=0.2, \n",
    "                                                    stratify=y, random_state=42)\n",
    "\n",
    "# Model Eğitimi (Random Forest Kullanımı)\n",
    "model = RandomForestClassifier(random_state=42, class_weight=\"balanced\")\n",
    "model.fit(X_train, y_train)\n",
    "\n",
    "# Test setinde tahmin yap\n",
    "y_pred = model.predict(X_test)\n",
    "y_pred_proba = model.predict_proba(X_test)[:, 1]\n",
    "\n",
    "# Performans değerlendirme\n",
    "print(\"Confusion Matrix:\")\n",
    "print(confusion_matrix(y_test, y_pred))\n",
    "\n",
    "print(\"\\nClassification Report:\")\n",
    "print(classification_report(y_test, y_pred))\n",
    "\n",
    "roc_auc = roc_auc_score(y_test, y_pred_proba)\n",
    "print(f\"ROC-AUC Skoru: {roc_auc:.4f}\")\n",
    "\n",
    "# Çapraz Doğrulama ve Hiperparametre Optimizasyonu\n",
    "param_grid = {\n",
    "    'n_estimators': [50, 100, 200],\n",
    "    'max_depth': [3, 5, 10],\n",
    "    'min_samples_split': [2, 5, 10]\n",
    "}\n",
    "\n",
    "grid_search = GridSearchCV(RandomForestClassifier(random_state=42, class_weight=\"balanced\"),\n",
    "                           param_grid, cv=5, scoring='roc_auc', n_jobs=-1)\n",
    "\n",
    "grid_search.fit(X_selected, y)\n",
    "\n",
    "print(\"En İyi Hiperparametreler:\", grid_search.best_params_)\n",
    "print(\"En İyi Çapraz Doğrulama Skoru:\", grid_search.best_score_)\n",
    "\n",
    "# En iyi modeli kullanarak test setini tekrar değerlendirme\n",
    "best_model = grid_search.best_estimator_\n",
    "y_pred_best = best_model.predict(X_test)\n",
    "y_pred_proba_best = best_model.predict_proba(X_test)[:, 1]\n",
    "\n",
    "print(\"\\nEn İyi Model ROC-AUC Skoru:\")\n",
    "print(roc_auc_score(y_test, y_pred_proba_best))\n",
    "\n",
    "# Korelasyon Analizi (Ekstra)\n",
    "plt.figure(figsize=(12, 10))\n",
    "sns.heatmap(data.corr(), annot=True, fmt=\".2f\", cmap=\"coolwarm\")\n",
    "plt.title(\"Korelasyon Matrisi\")\n",
    "plt.show()"
   ]
  }
